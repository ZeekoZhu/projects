{
  "name": "gsi-overlay-tool",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/gsi-overlay-tool",
  "targets": {
    "build": {
      "executor": "@nx-dotnet/core:build",
      "outputs": [
        "{workspaceRoot}/dist/apps/gsi-overlay-tool",
        "{workspaceRoot}/dist/intermediates/apps/gsi-overlay-tool"
      ],
      "options": {
        "configuration": "Debug",
        "noDependencies": true,
        "extraParameters": "--runtime linux-x64"
      },
      "configurations": {
        "production": {
          "configuration": "Release"
        },
        "android": {
          "configuration": "Release",
          "extraParameters": "--runtime linux-bionic-arm64"
        }
      }
    },
    "serve": {
      "executor": "@nx-dotnet/core:serve",
      "options": {
        "configuration": "Debug"
      },
      "configurations": {
        "production": {
          "configuration": "Release"
        }
      }
    },
    "publish": {
      "executor": "@nx-dotnet/core:publish",
      "outputs": [
        "{workspaceRoot}/dist/publish/gsi-overlay-tool"
      ],
      "dependsOn": [
        "build"
      ],
      "options": {
        "configuration": "Debug",
        "noBuild": true,
        "output": "dist/publish/gsi-overlay-tool",
        "runtime": "linux-x64"
      },
      "configurations": {
        "production": {
          "configuration": "Release"
        },
        "android": {
          "configuration": "Release",
          "runtime": "linux-bionic-arm64"
        }
      }
    },
    "lint": {
      "executor": "@nx-dotnet/core:format"
    }
  },
  "tags": []
}
