<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <!-- Add Resources Here --><ResourceDictionary.MergedDictionaries>
    <ResourceInclude Source="/Colors.axaml" />
  </ResourceDictionary.MergedDictionaries>
  <SolidColorBrush x:Key="ControlBorderBrush" Color="{StaticResource Slate500}" />
  <SolidColorBrush x:Key="ControlBackgroundBrush" Color="Transparent" />
  <SolidColorBrush x:Key="ControlForegroundBrush" Color="{StaticResource Slate900}" />

  <SolidColorBrush x:Key="BrushOnSecondaryRed" Color="{StaticResource Red900}"></SolidColorBrush>
  <SolidColorBrush x:Key="BackgroundSecondaryRed" Color="{StaticResource Red50}"></SolidColorBrush>
  <SolidColorBrush x:Key="BackgroundSecondaryHoverRed" Color="{StaticResource Red200}"></SolidColorBrush>
  <SolidColorBrush x:Key="BackgroundSecondaryPressedRed" Color="{StaticResource Red400}"></SolidColorBrush>
  <SolidColorBrush x:Key="BrushOnSecondaryGreen" Color="{StaticResource Green900}"></SolidColorBrush>
  <SolidColorBrush x:Key="BackgroundSecondaryGreen" Color="{StaticResource Green50}"></SolidColorBrush>
  <SolidColorBrush x:Key="BackgroundSecondaryHoverGreen" Color="{StaticResource Green200}"></SolidColorBrush>
  <SolidColorBrush x:Key="BackgroundSecondaryPressedGreen" Color="{StaticResource Green400}"></SolidColorBrush>
</ResourceDictionary>
